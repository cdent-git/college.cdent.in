   <script>
       
       
   $("#btn-remove-default").hide();
    let extra = 0;
        $("#btn-add").click(() => {
          extra++;
          
          let html  = $("#form-wrapper").html();
          let nextID = 'extra-dept-' + extra;
          html = '<div class="col-xl-3 col-md-6" id="' + nextID + '">' + html + '</div>';
          $("#main-wrapper").append(html);

          let rBtnID = 'btn-' + nextID;
          let selector = '#' + nextID + ' div.card-body button.close-btn';
          console.log(selector);
          let onclick = "removeExtraDept('#" + nextID + "');" ;
          $(selector).prop("id", rBtnID).css("display", "block");//.prop("onclick", onclick);

          $(selector).on("click", () => {
              removeExtraDept('#'+ nextID);
          });


    });
    
    const removeExtraDept = (id) => {
       $(id).remove();
    }
    
  
   
   
   </script>

